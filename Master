#include <stdio.h>
#include <math.h>

int main()
{
float principal, rate, time, CI;

printf("Enter principal (amount): ");
scanf("%f", &principal);

printf("Enter time: ");
scanf("%f", &time);

printf("Enter rate: ");
scanf("%f", &rate);
CI = principle* (pow((1 + rate / 100), time));

printf("Compound Interest = %f", CI);
return 0;
}
